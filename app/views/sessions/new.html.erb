<h1> Login</h1>
<br>
<% if flash[:notice] %>
<h6><%= flash[:notice] %></h6>
<%end%>
<br>

    <%= form_tag("/login") do %><br>
        <%= label_tag "Username:" %><br>
        <%= text_field_tag :username %><br>

        <%= label_tag "Password:" %><br>
        <%= password_field_tag :password %><br>
        <%= submit_tag "Login!" %>
    <% end %>
    <br>
    <%= button_to "Log in With Google", '/auth/google_oauth2' %>